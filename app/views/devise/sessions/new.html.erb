<div style="margin-left:55px;"><%= image_tag("machinelogo.jpg", :alt => "camp",:class=>"logo") %></div>
<div class="headerline"></div>
<div style="margin-left:55px;">
<h2>Sign in</h2>

<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
  <p><%= f.label :email%><br />
  <%= f.email_field :email,:class=>"round"  %></p>

  <p><%= f.label :password %><br />
  <%= f.password_field :password , :class=>"round"  %></p>

  <% if devise_mapping.rememberable? -%>
    <span><%= f.check_box :remember_me %> <%= f.label :remember_me %></span>
  <% end -%>

  <p><%= f.submit "Sign in", :class=>"sign_up_button" %></p>
<% end %>

<%= render :partial => "devise/shared/links" %>
</div>
<div class="footerline"></div>
